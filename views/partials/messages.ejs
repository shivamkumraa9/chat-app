<% if(messages.success){ %>
<% for (i = 0; i < messages.success.length; i++){ %>
	<div class="alert alert-success" role="alert">
	  <%= messages.success[i] %>
	</div>
<% }} %>


<% if(messages.failure){ %>
<% for (i = 0; i < messages.failure.length; i++){ %>
	<div class="alert alert-danger" role="alert">
	  <%= messages.failure[i] %>
	</div>
<% }} %>